if [ "$TMPDIR" = "" ]; then
  local tmp_download_dir=$(mktemp -d)
else
  local tmp_download_dir=$TMPDIR
fi

get_download_url() {
  local version=$1
  echo "https://github.com/wg/wrk/archive/${version}.tar.gz"
}

cd $tmp_download_dir

local download_url=$(get_download_url $ASDF_INSTALL_VERSION)
curl -L $download_url | tar zxv
make

ls -alsh
